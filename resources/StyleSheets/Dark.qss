QTreeView
{
	background-color: #1e1e1e;
	color: #f0f0f0;
	border-style: solid;
	border-color: #323232;
	border-width: 1px;
	outline: none;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings 
{
	border-image: none;
	image: url(":/Controls/arrow-point-to-right-white.png");
}

QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover 
{
	border-image: none;
	image: url(":/Controls/arrow-point-to-right-blue.png");
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings
{
	border-image: none;
	image: url(":/Controls/arrow-point-to-down-white.png");
}

QTreeView::branch:open:has-children:!has-siblings:hover,
QTreeView::branch:open:has-children:has-siblings:hover
{
	border-image: none;
	image: url(":/Controls/arrow-point-to-down-blue.png");
}


