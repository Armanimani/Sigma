QDockWidget > QWidget
{
	background-color: #1e1e1e;
	border-style: solid;
	border-width: 1px;
	border-color: #323232;
	color: #f0f0f0;
	titlebar-close-icon: url(":/Icons/window_close_32.png");
	titlebar-normal-icon: url(":/Icons/window_maximize_32.png");
}

QDockWidget::close-button, QDockWidget::float-button {
    border: none;
    background: transparent;
    icon-size: 30px;
    padding: 1px;
}


QDockWidget::float-button {
    image: url(":/Icons/window_maximize_32.png");
}

QDockWidget::close-button {
    image: url(":/Icons/window_close_32.png");
}


QDockWidget::title 
{
	border-style: solid;
	border-width: 1px;
	border-color: #404040;
	text-align: left;
	background-color: #323232;
	width: 32px;
}


